{
  "name": "flappy-bird-backend",
  "version": "1.0.0",
  "description": "Backend for Flappy Bird game with leaderboard",
  "main": "dist/index.js",
  "scripts": {
    "dev": "prisma generate && ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "express": "^4.18.0",
    "socket.io": "^4.7.0",
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "typescript": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/node": "^20.0.0",
    "@types/socket.io": "^3.0.0",
    "ts-node": "^10.9.0"
  }
}